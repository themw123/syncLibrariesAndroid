<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="circleImageViewException" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">0%</item>

    </style>

    <style name="circleImageViewDownload" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>

    </style>

    <style name="switch1" >
        <item name="colorControlActivated">#246928</item>
        <item name="android:colorForeground">#ffffff</item>
    </style>

    <style name="TabLayoutStyle">
        <item name="android:textSize">15dp</item>
    </style>


    <style name="AlertDialogTheme">
        <item name="buttonBarPositiveButtonStyle">@style/Alert.Button.Positive</item>
        <item name="buttonBarNeutralButtonStyle">@style/Alert.Button.Neutral</item>
    </style>

    <style name="Alert.Button.Positive" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="backgroundTint">#3584D5</item>
        <item name="rippleColor">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Alert.Button.Neutral" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="backgroundTint">#3584D5</item>
        <item name="rippleColor">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
    </style>





</resources>